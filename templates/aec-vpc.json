                "CidrIp": {
                    "Ref": "VPCCIDR"
                }
            }
        },
        "ingress8": {
            "Type": "AWS::EC2::SecurityGroupIngress",
            "Properties": {
                "GroupId": {
                    "Ref": "AECPrivateGroup"
                },
