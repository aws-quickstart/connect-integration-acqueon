                "FromPort": "5555",
                "ToPort": "5555",
                "CidrIp": {
                    "Ref": "VPCCIDR"
                }
            }
        },
        "ingress9": {
            "Type": "AWS::EC2::SecurityGroupIngress",
            "Properties": {
                "GroupId": {
                    "Ref": "AECPrivateGroup"
                },
